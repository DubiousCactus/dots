[global_config]
  enabled_plugins = TerminalShot, LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  focus = system
  inactive_color_offset = 0.72
  sticky = True
  title_font = Monaco 13
  title_hide_sizetext = True
  title_transmit_bg_color = "#2e3436"
  title_transmit_fg_color = "#8f5902"
  title_use_system_font = False
  window_state = maximise
[keybindings]
  copy = <Primary>c
  paste = <Primary>v
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    audible_bell = True
    background_color = "#002b36"
    background_darkness = 0.87
    cursor_color = "#dcdcdc"
    cursor_shape = ibeam
    font = Monaco 11
    foreground_color = "#839496"
    palette = "#073642:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#eee8d5:#002b36:#cb4b16:#586e75:#657b83:#839496:#6c71c4:#93a1a1:#fdf6e3"
    scrollback_infinite = True
    use_system_font = False
  [[Custom]]
    background_color = "#23202f"
    cursor_color = "#25C3DC"
    font = Fira Code 11
    use_system_font = False
  [[material-dark]]
    cursor_color = "#aaaaaa"
  [[material-light]]
    background_color = "#eeeeee"
    cursor_color = "#25C3DC"
    foreground_color = "#3c3c3b"
    palette = "#f8f8f8:#d7005f:#718c00:#ff8f01:#005f87:#6a1e9a:#3e999f:#2c2c2c:#2c2c2c:#ef5251:#90c94d:#ffa000:#4271ae:#c774c1:#3e999f:#f5f5f5"
  [[zenburn]]
    cursor_color = "#aaaaaa"
    cursor_shape = ibeam
